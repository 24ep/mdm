{
  "name": "customer-data-management",
  "version": "1.2.3-beta.6",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--max-old-space-size=8192 next dev --webpack",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 prisma generate && cross-env NODE_OPTIONS=--max-old-space-size=8192 next build --webpack",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "postinstall": "prisma generate",
    "scan:ts": "node scripts/scan-typescript-errors.js",
    "scan:ts:check": "node scripts/scan-typescript-errors.js && echo TypeScript check passed",
    "scan:all": "node scripts/scan-all-issues.js",
    "update-themes": "tsx prisma/update-themes.ts",
    "remove-default-light": "tsx prisma/remove-default-light.ts",
    "migrate-drawer-style": "tsx prisma/migrate-drawer-style.ts",
    "migrate-icon-button": "tsx prisma/migrate-icon-button.ts",
    "update-badge-theme": "node scripts/update-badge-theme-config.js",
    "seed:themes": "tsx src/scripts/seed-themes.ts",
    "migrate-api-routes": "node scripts/migrate-api-routes.js",
    "migrate-api-routes:dry": "node scripts/migrate-api-routes.js --dry-run",
    "migrate-api-routes:test": "node scripts/migrate-api-routes.js --dry-run --limit 5",
    "seed": "npx prisma db push --force-reset --accept-data-loss && node -r dotenv/config scripts/run-migration.js && tsx scripts/create-admin-user.ts && tsx prisma/seed-assets.ts && tsx prisma/seed-storage-connection.ts && node -r dotenv/config scripts/seed-plugins.js && tsx src/scripts/seed-themes.ts && tsx scripts/seed-system-settings.ts && node -r dotenv/config scripts/seed-global-roles.js && tsx src/scripts/seed-platform-menu.ts"
  },
